create table area (radius int , area float);

delimiter //

create procedure area_calc()
begin
  declare r int;
  declare a float;
  set r = 5;
  while r<=9 do
    set a = 3.14 * r * r;
    insert into area values (r, a);
    set r=r+1;
  end while;
end; //

delimiter ;

call area_calc();
select * from area;
